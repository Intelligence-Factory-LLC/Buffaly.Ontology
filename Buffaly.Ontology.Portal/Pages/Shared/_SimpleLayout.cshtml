@{
    var vmodel = new Buffaly.Admin.Portal.Pages.Shared._TopBarModel();
}
<!DOCTYPE html>
<html lang="en">

    <head>

        <meta charset="utf-8" />
        <title>@ViewContext.ViewData["Title"]</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta content="Semantic Database" name="description" />
        <meta content="Coderthemes" name="author" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <!-- App favicon -->
        <link rel="shortcut icon" href="~/images/favicon.ico">

        @RenderSection("Styles", false)

    <link href="~/lib/jquery-toast-plugin/jquery.toast.min.css" rel="stylesheet" type="text/css" />

        <!-- Bootstrap css -->
        <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <!-- App css -->
        <link href="~/css/app.min.css" rel="stylesheet" type="text/css" id="app-style"/>
        <!-- icons -->
        <link href="~/css/icons.min.css" rel="stylesheet" type="text/css" />
        <!-- Head js -->
        <script src="~/js/head.js"></script>


	<link href="~/css/styles.css" rel="stylesheet" type="text/css" />


        <!--kcs js-->
    <script src="//static.buffa.ly/js/global.js"></script>
	<script src="//static.buffa.ly/js/LocalStorage2.js"></script>
	<script src="//static.buffa.ly/js/moremoo3.js?v=2"></script>
	<script src="//static.buffa.ly/js/validators.js?v=2"></script>
	<script src="//static.buffa.ly/js/UserMessages.js"></script>
	<script src="//static.buffa.ly/js/grid4.js"></script>
	<script src="//static.buffa.ly/js/stl.js?v=2"></script>
	<script src="//static.buffa.ly/js/gridselector.js"></script>
	<script src="//static.buffa.ly/js/moment.min.js"></script>
	<script src="//static.buffa.ly/js/DateUtil.js"></script>
	<script src="//static.buffa.ly/js/form_validation.js"></script>

	<script src="~/js/kcs/MasterPage.js"></script>
    <link href="~/css/site.css" rel="stylesheet" type="text/css" />
	<script src="~/js/plugins/tinymce/tinymce.min.js"></script>



	<script type="text/javascript" src="~/js/plugins/codemirror/lib/codemirror.js"></script>
	<link rel="stylesheet" href="~/js/plugins/codemirror/lib/codemirror.css">
	<script src="~/js/plugins/codemirror/mode/javascript/javascript.js"></script>
	<script src="/js/plugins/codemirror/mode/xml/xml.js"></script>
	<script src="/js/plugins/codemirror/mode/css/css.js"></script>
	<script src="/js/plugins/codemirror/mode/htmlmixed/htmlmixed.js"></script>
	<script src="/js/plugins/codemirror/mode/markdown/markdown.js"></script>
	<script src="/js/plugins/marked/marked.min.js"></script>


    <style>
        .CodeMirror {
            border: 1px solid #eee;
            height: auto;
        }
    </style>

	@RenderSection("Head", false)

    </head>

    <!-- body start -->
    <body data-layout-mode="default" data-theme="light" data-layout-width="fluid" data-topbar-color="light" data-menu-position="fixed" data-leftbar-color="light" data-leftbar-size='default' data-sidebar-user='false'>

        <!-- Begin page -->
        <div id="wrapper">


        <partial model="vmodel" name="_TopBar"></partial>
		<div class="navbar-custom" id="topbar">
    <div class="container-fluid">
        <ul class="list-unstyled topnav-menu float-end mb-0">

          


 
        </ul>

        <!-- LOGO -->
        <div class="logo-box">
            <a href="/" class="logo logo-dark text-center">
                <span class="logo-sm">
                    <img src="~/images/logo-sm.png" alt="" height="22">
               
                </span>
                <span class="logo-lg">
                    <img src="@Model.Logo" alt="" height="50">
               
                </span>
            </a>

            <a href="/" class="logo logo-light text-center">
                <span class="logo-sm">
                    <img src="~/images/logo-sm.png" alt="" height="22">
                </span>
                <span class="logo-lg">
                    <img src="~/images/logo-light.png" alt="" height="20">
                </span>
            </a>
        </div>

		@RenderSection("FileMenu", false)

        <div class="clearfix"></div>
    </div>
</div>

<div class="left-side-menu" id="sidebar">

    <div class="h-100" data-simplebar>

        <!-- User box -->
        

        <div id="sidebar-menu">
		@RenderSection("SidebarMenu", false)
		@if (!IsSectionDefined("SidebarMenu"))
		{
			<p>Default content for sidebar menu</p>
		}
        </div>

        <div class="clearfix"></div>

    </div>
    <!-- Sidebar -left -->

</div>


            <div class="content-page">
 
            <div class="content">
                <!-- Start Content-->
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row" id="divPageTitle">
                        <div class="col-12">
                            <div class="page-title-box">
                                <div class="page-title-right">
                                    @RenderSection("Breadcrumbs", false)
								</div>
                                <h4 class="page-title">@RenderSection("MainPanelTitle", false)</h4>
								@RenderSection("MainPanelTitleOptions", false)
                            </div>
                        </div>
                    </div>
                    <!-- end page title -->
					@RenderSection("ErrorMessage", false)
                    <div class="row">
                        <div class="col-12">
                            @RenderSection("MainPanelContent", false)
                        </div> <!-- end col-->
                    </div>
                    <!-- end row-->

                </div>
                <!-- container -->
            </div>

                <partial name="_Footer"></partial>

            </div>


        </div>
        <!-- END wrapper -->

         <partial name="_RightSidebarEmpty"></partial>


        <!-- Vendor js -->
        <script src="~/js/vendor.min.js"></script>


        @RenderSection("scripts", false)
        <!-- App js-->
        <script src="~/js/app.min.js"></script>
        <script src="~/lib/jquery-toast-plugin/jquery.toast.min.js"></script>


        @RenderSection("BottomScripts", false)



    </body>
</html>